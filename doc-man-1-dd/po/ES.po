msgstr "Conversiones:"

#: 1-dd.xml:196
msgid "<emphasis remap=\"B\">ascii</emphasis>"
msgstr "<emphasis remap=\"B\">ascii</emphasis>"

#: 1-dd.xml:198
msgid "Convert EBCDIC to ASCII."
msgstr "Convierte EBCDIC a ASCII."

#: 1-dd.xml:202
msgid "<emphasis remap=\"B\">ebcdic</emphasis>"
msgstr "<emphasis remap=\"B\">ebcdic</emphasis>"

#: 1-dd.xml:204
msgid "Convert ASCII to EBCDIC."
msgstr "Convierte ASCII a EBCDIC."

#: 1-dd.xml:208
msgid "<emphasis remap=\"B\">ibm</emphasis>"
msgstr "<emphasis remap=\"B\">ibm</emphasis>"

#: 1-dd.xml:210
msgid "Convert ASCII to alternate EBCDIC."
msgstr "Convierte ASCII a un EBCDIC alternativo."

#: 1-dd.xml:214
msgid "<emphasis remap=\"B\">block</emphasis>"
msgstr "<emphasis remap=\"B\">block</emphasis>"

#: 1-dd.xml:216
msgid "For each line in the input, output <emphasis remap=\"B\">cbs</emphasis> bytes, replacing the input newline with a space and padding with spaces as necessary."
msgstr "Para cada línea de la entrada, saca <emphasis remap=\"B\">cbs</emphasis> bytes, reemplazando el salto de línea de la entrada con un espacio y rellenando con más espacios si fuera necesario."

#: 1-dd.xml:223
msgid "<emphasis remap=\"B\">unblock</emphasis>"
msgstr "<emphasis remap=\"B\">unblock</emphasis>"

#: 1-dd.xml:225
msgid "Replace trailing spaces in each <emphasis remap=\"B\">cbs</emphasis>-sized input block with a newline."
msgstr "Reemplaza espacios del final en cada bloque de entrada de tamaño determinado por <emphasis remap=\"B\">cbs</emphasis> por un salto de línea."

#: 1-dd.xml:231
msgid "<emphasis remap=\"B\">lcase</emphasis>"
msgstr "<emphasis remap=\"B\">lcase</emphasis>"

#: 1-dd.xml:233
msgid "Change uppercase letters to lowercase."
msgstr "Cambia las letras mayúsculas por minúsculas."

#: 1-dd.xml:237
msgid "<emphasis remap=\"B\">ucase</emphasis>"
msgstr "<emphasis remap=\"B\">ucase</emphasis>"

#: 1-dd.xml:239
msgid "Change lowercase letters to uppercase."
msgstr "Cambia las letras minúsculas por mayúsculas."

#: 1-dd.xml:243
msgid "<emphasis remap=\"B\">swab</emphasis>"
msgstr "<emphasis remap=\"B\">swab</emphasis>"

#: 1-dd.xml:245
msgid "Swap every pair of input bytes. If an odd number of bytes are read the last byte is simply copied (since there is nothing to swap it with). [POSIX 1003.2b, PASC interpretations 1003.2 #3 and #4]"
msgstr "Intercambia cada par de bytes de la entrada. Si se lee un número impar de bytes el último byte se copia tal cual (ya que no tiene con quién intercambiarse). [POSIX 1003.2b, PASC interpretaciones 1003.2 nº 3 y nº 4]"

#: 1-dd.xml:252
msgid "<emphasis remap=\"B\">noerror</emphasis>"
msgstr "<emphasis remap=\"B\">noerror</emphasis>"

#: 1-dd.xml:254
msgid "Continue after read errors."
msgstr "Continúa despues de producirse errores de lectura."

#: 1-dd.xml:258
msgid "<emphasis remap=\"B\">notrunc</emphasis>"
msgstr "<emphasis remap=\"B\">notrunc</emphasis>"

#: 1-dd.xml:260
msgid "Do not truncate the output file."
msgstr "No trunca el fichero de salida."

#: 1-dd.xml:264
msgid "<emphasis remap=\"B\">sync</emphasis>"
msgstr "<emphasis remap=\"B\">sync</emphasis>"

#: 1-dd.xml:266
msgid "Pad every input block to size of <emphasis remap=\"B\">ibs</emphasis> with trailing zero bytes."
msgstr "Rellena cada bloque de entrada hasta el tamaño determinado por <emphasis remap=\"B\">ibs</emphasis> con bytes cero al final."

